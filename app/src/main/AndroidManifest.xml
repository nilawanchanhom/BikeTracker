<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.nilawan.backup2" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <!--
 The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but are recommended.
    -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/red"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".ConnectLogin"
            android:label="@string/title_activity_maps" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MapsActivity"
            android:label="@string/app_name"/>
        <activity android:name=".AlarmActivity"
            android:label="@string/app_name"/>
        <activity android:name=".MainActivity"
            android:label="@string/app_name"/>
        <activity android:name=".Notification"
            android:label="@string/app_name"/>
        <activity android:name=".NotificationAlarm"
            android:label="@string/app_name"/>
        <activity android:name=".ControlActivity"
            android:label="@string/app_name"/>
        <activity android:name=".Connectmotion"
        android:label="@string/app_name"/>
        <activity android:name=".Login"
            android:label="@string/app_name"/>
        <activity android:name=".sendAlarmOn"
            android:label="@string/app_name"/>
        <activity android:name=".sendAlarmOff"
            android:label="@string/app_name"/>
        <activity android:name=".ConnectStatusAl"
            android:label="@string/app_name"/>
        <activity android:name=".Lock"
            android:label="@string/app_name"/>
        <activity android:name=".SendLock"
            android:label="@string/app_name"/>
        <activity android:name=".SendUnlock"
            android:label="@string/app_name"/>
        <activity android:name=".Connectmotion2"
            android:label="@string/app_name"/>
        <activity android:name=".CheckNoti"
            android:label="@string/app_name"/>
        <service android:name=".MyService"
            android:exported="false">
        </service>
    </application>

</manifest>
